<h2>Queues</h2>
<h3>Overview</h3>

<div class="row">
  <div class="col-sm-4 no-padding">
    <ul class="list-group">
      {{#each queues}}
        <li class="list-group-item">
          <a href="{{ ../baseURL }}/dashboard/{{ encodeURI this.hostId }}/{{ encodeURI this.name }}">{{ this.hostId }}/{{ this.name }}</a>
        </li>
      {{/each}}
    </ul>
  </div>
</div>

{{#contentFor 'sidebar'}}
  <li class="active"><a href="#">Queues Overview</a></li>
{{/contentFor}}
